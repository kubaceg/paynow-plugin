<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>

        <defaults public="true" autoconfigure="false" autowire="false"/>

        <service id="kubaceg.paynow.form.type.gateway_configuration" class="Kubaceg\SyliusPaynowPlugin\Form\PaynowGatewayConfigurationType">
            <tag name="kubaceg.paynow.gateway_form" type="paynow" label="kubaceg.paynow.paynow"/>
            <tag name="form.type"/>
        </service>
    </services>
</container>
